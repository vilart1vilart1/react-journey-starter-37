<job-scheduling-data xmlns="http://quartznet.sourceforge.net/JobSchedulingData"
										xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
										version="2.0">

	<processing-directives>
		<overwrite-existing-data>true</overwrite-existing-data>
	</processing-directives>

	<pre-processing-commands>
		<delete-trigger>
			<name>AddressGeocodingAgentTrigger</name>
			<group>Core</group>
		</delete-trigger>
		<delete-trigger>
			<name>ServiceOrderGeocodingAgentTrigger</name>
			<group>Crm.Service</group>
		</delete-trigger>

		<delete-trigger>
			<name>DropboxAgentTrigger</name>
			<group>Core</group>
		</delete-trigger>

		<delete-trigger>
			<name>ApplicationStartEmailAgentTrigger</name>
			<group>Core</group>
		</delete-trigger>
		<delete-job>
			<name>EmailAgent</name>
			<group>Core</group>
		</delete-job>

		<delete-trigger>
			<name>ServiceOrderDocumentSaverAgentTrigger</name>
			<group>Crm.Service</group>
		</delete-trigger>
		<delete-job>
			<name>ServiceOrderDocumentSaverAgent</name>
			<group>Crm.Service</group>
		</delete-job>

		<delete-trigger>
			<name>DispatchDocumentSaverAgentTrigger</name>
			<group>Crm.Service</group>
		</delete-trigger>
		<delete-job>
			<name>DispatchDocumentSaverAgent</name>
			<group>Crm.Service</group>
		</delete-job>
	</pre-processing-commands>

	<schedule>

	<job>
		<name>DispatchDocumentSaverAgent</name>
		<group>Customer.Swecon</group>
		<description>Saves dispatch reports to the filesystem</description>
		<job-type>Customer.Swecon.BackgroundServices.DispatchDocumentSaverAgent, Customer.Swecon</job-type>
		<durable>true</durable>
		<recover>false</recover>
	</job>
	<trigger>
		<cron>
			<name>DispatchDocumentSaverAgentTrigger</name>
			<group>Customer.Swecon</group>
			<description>Runs every minute to save unsaved reports</description>
			<job-name>DispatchDocumentSaverAgent</job-name>
			<job-group>Customer.Swecon</job-group>
			<cron-expression>0 0/5 * * * ?</cron-expression>
		</cron>
	</trigger>
	
	
		<job>
			<name>ServiceOrderNoteSaverAgent</name>
			<group>Customer.Swecon</group>
			<description>Saves service order notes to the filesystem</description>
			<job-type>Customer.Swecon.BackgroundServices.ServiceOrderNoteSaverAgent, Customer.Swecon</job-type>
			<durable>true</durable>
			<recover>false</recover>
		</job>
		<trigger>
			<cron>
				<name>ServiceOrderNoteSaverAgentTrigger</name>
				<group>Customer.Swecon</group>
				<description>Runs every minute to save unsaved reports</description>
				<job-name>ServiceOrderNoteSaverAgent</job-name>
				<job-group>Customer.Swecon</job-group>
				<cron-expression>0 0/5 * * * ?</cron-expression>
			</cron>
		</trigger>
	
	
	</schedule>

</job-scheduling-data>